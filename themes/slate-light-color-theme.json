      }
    },
    {
      "scope": [
        "keyword.other"
      ],
      "settings": {
        "foreground": "#5C5485"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#000000DF"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
