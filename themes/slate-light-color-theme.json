  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#9090A8"
      }
    },
    {
      "scope": [
        "comment.block.documentation",
        "comment.block.javadoc"
      ],
      "settings": {
        "foreground": "#9090A8"
      }
    },
    {
      "scope": [
        "constant",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#0F6E80"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.other"
      ],
      "settings": {
        "foreground": "#3A5CB0"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#1C6E48"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#0F6E80"
      }
    },
    {
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#2E527A"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
