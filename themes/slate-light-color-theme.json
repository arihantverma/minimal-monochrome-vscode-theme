      "settings": {
        "foreground": "#0F6E80"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "storage.type",
        "storage.modifier"
