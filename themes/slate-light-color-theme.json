        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#000000DF"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#000000DF"
      }
    },
    {
