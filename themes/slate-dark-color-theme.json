        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
    {
      "scope": [
        "punctuation.bracket",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#EEEEEE"
