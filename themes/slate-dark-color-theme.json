  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#515166"
      }
    },
    {
      "scope": [
        "comment.block.documentation",
        "comment.block.javadoc"
      ],
      "settings": {
        "foreground": "#515166"
      }
    },
    {
      "scope": [
        "constant",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#89A7B1"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.other"
      ],
      "settings": {
        "foreground": "#9eb2d9"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#CBDAD5"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#89A7B1"
      }
    },
    {
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#566981"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#89A7B1"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#566981"
      }
    },
    {
      "scope": [
        "keyword.other"
      ],
      "settings": {
        "foreground": "#686883"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
